        "id": "photo-intro",
        "type": "blocks",
        "enabled": true,
        "data": {
          "container": {
            "enabled": true,
            "className": "bg-neutral-200 pt-28 pb-24 border-b border-neutral-900"
          },
          "innerWrapper": {
            "enabled": true,
            "className": "container mx-auto px-6 lg:px-10 space-y-10"
          },
          "blocks": [
            {
              "type": "paragraph",
              "enabled": true,
              "text": "LA-BASED PHOTOGRAPHY BY SARA KARAZI",
              "size": "sm",
              "align": "left",
              "className": "font-serif text-xs tracking-[0.28em] uppercase text-neutral-900"
            },
            {
              "type": "image",
              "enabled": true,
              "src": "https://images.unsplash.com/photo-1516035069371-29a1b244cc32?auto=format&fit=crop&w=2200&q=80",
              "alt": "Editorial portrait in studio light",
              "size": "lg",
              "className": "aspect-[16/7] rounded-none border-neutral-900"
            },
            {
              "type": "paragraph",
              "enabled": true,
              "text": "Editorial portraiture with a restrained palette, precise composition, and seasonal release drops.",
              "size": "sm",
              "align": "left",
              "className": "font-serif text-sm leading-7 text-neutral-800"
            },
            {
              "type": "buttonRow",
              "enabled": true,
              "size": "sm",
              "align": "left",
